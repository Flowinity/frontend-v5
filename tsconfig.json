{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "useDefineForClassFields": true,
    "allowSyntheticDefaultImports": true,
    "composite": false,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": false,
    "jsx": "preserve",
    "sourceMap": false,
    "resolveJsonModule": true,
    "noEmit": true,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "lib": ["esnext", "DOM", "DOM.Iterable", "webworker"],
    "skipLibCheck": true,
    "types": ["node", "vuetify", "vite-plugin-pwa/client"],
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": [
    "package.json",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "../types/**/*.d.ts",
    "../models/**/*.ts",
    "vite.config.ts",
    "*.vue",
    "../../../../../../opt/webstorm/plugins/javascript-impl/jsLanguageServicesImpl/external/lib.dom.d.ts"
  ],
  "exclude": [
    "src/components/Workspaces/EditorV2/**"
  ]
}
